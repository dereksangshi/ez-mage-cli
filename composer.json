{
    "name": "ez/mage-cli",
    "type": "library",
    "description": "MageCli",
    "keywords": ["magento", "cli"],
    "homepage": "https://bitbucket.org/dereksangshi2000/ez-mage-cli",
    "license": "MIT",
    "authors": [
        {"name": "Derek Li", "email": "dereksangshi2000@gmail.com.au"}
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "~2.0",
        "ez/mage-bridge": "1.*",
        "ez/data-structure": "1.*",
        "ez/util": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.internal.balancenet.com.au"
        }
    ],
    "autoload": {
        "psr-0": { "Ez\\MageCli\\": "src/" }
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml", ".travis.yml", "build.xml", "build.properties", "composer.phar"]
    }
}
